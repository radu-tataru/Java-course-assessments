{
  "buildCommand": "",
  "outputDirectory": ".",
  "installCommand": "npm install",
  "functions": {
    "api/submissions.js": {
      "maxDuration": 30
    },
    "api/auth.js": {
      "maxDuration": 30
    },
    "api/assessments.js": {
      "maxDuration": 30
    },
    "api/init-db.js": {
      "maxDuration": 60
    }
  },
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET, POST, PUT, OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Content-Type, Authorization"
        }
      ]
    }
  ]
}